<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="Adopt.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/index.jsp"></property>
	</bean>
	
	<bean id="Adopt.success" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/view.jsp"></property>
	</bean>
</beans>
